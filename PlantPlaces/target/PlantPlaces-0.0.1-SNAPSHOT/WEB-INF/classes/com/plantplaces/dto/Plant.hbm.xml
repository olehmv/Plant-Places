<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.plantplaces.dto.Plant" table="plants" catalog="plantplaces">
		<id id="guid" type="java.lang.Integer">
			<column name="PLANT_ID"></column>
			<generator class="identity" />
		</id>
		<property  name="genus" type="java.lang.String">
			<column name="GENUS" lenght="255" not-null="false" unique="false"></column>
		</property>
		<property name="species" type="java.lang.String">
			<column name="SPECIES" lenght="255" not-null="false" unique="false"></column>
		</property>
		<property  name="common" type="java.lang.String">
			<column name="COMMON" lenght="255" not-null="false" unique="false"></column>
		</property>
		<property  name="cultivar" type="java.lang.String">
			<column name="CULTIVAR" lenght="255" not-null="false" unique="false"></column>
		</property>

	</class>
</hibernate-mapping>


	